{
  "name": "app",
  "version": "1.0.0",
  "description": "Web application to consume @react-css libraries",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "webpack serve --mode development --port 40000 --progress --hot",
    "clean": "rm -rf dist/",
    "update": "yarn upgrade --dev --scope @react-css --latest",
    "build": "yarn clean && webpack --mode production",
    "serve": "yarn build && serve dist --listen 40000 --no-clipboard",
    "ci": "yarn update && yarn serve",
    "lint": "tsc --noEmit && eslint *.{js,ts,tsx} --quiet --fix"
  },
  "dependencies": {
    "@react-css/flex": "^1.4.3",
    "@react-css/grid": "^1.4.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-loader": "^8.2.2",
    "clsx": "^1.1.1",
    "compression-webpack-plugin": "^7.1.2",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "milligram": "^1.4.1",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
