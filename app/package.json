{
  "name": "app",
  "version": "1.1.0",
  "description": "Web application to consume @react-css libraries",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "webpack serve --mode development --port 40000 --progress --hot",
    "clean": "rm -rf dist/",
    "update": "yarn upgrade --scope @react-css --latest",
    "build": "yarn clean && webpack --mode production",
    "serve": "yarn build && serve dist --listen 40000 --no-clipboard",
    "ci": "yarn update && yarn serve",
    "lint": "tsc --noEmit && eslint *.{js,ts,tsx} --quiet --fix"
  },
  "dependencies": {
    "@react-css/flex": "^1.6.1",
    "@react-css/grid": "^1.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-loader": "^8.2.4",
    "clsx": "^1.1.1",
    "compression-webpack-plugin": "^9.2.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "milligram": "^1.4.1",
    "prettier": "^2.6.2",
    "serve": "^13.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
