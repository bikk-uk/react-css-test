{
  "name": "app",
  "version": "1.1.0",
  "description": "Web application to consume @react-css libraries",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "webpack serve --mode development --port 40000 --progress --hot",
    "clean": "rm -rf dist/",
    "update": "yarn upgrade --scope @react-css --latest",
    "build": "yarn clean && webpack --mode production",
    "serve": "yarn build && serve dist --listen 40000 --no-clipboard",
    "ci": "yarn update && yarn serve",
    "lint": "tsc --noEmit && eslint *.{js,ts,tsx} --quiet --fix"
  },
  "dependencies": {
    "@react-css/flex": "^1.4.6",
    "@react-css/grid": "^1.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-loader": "^8.2.2",
    "clsx": "^1.1.1",
    "compression-webpack-plugin": "^9.0.0",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "milligram": "^1.4.1",
    "prettier": "^2.4.1",
    "serve": "^12.0.1",
    "style-loader": "^3.3.0",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  }
}
